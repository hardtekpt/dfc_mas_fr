<?xml version="1.0"?>
<launch>
    <arg name="sim" default="true"/>

    <group ns="algorithm">
        <rosparam file="$(find dfc_mas_fr)/config/algorithm_params.yaml" />

        <node pkg="dfc_mas_fr" type="Publisher.py" name="publisher" output="screen" args="--sim --dt=0.25 --vis=mpl">
            
        </node>

        <include file="$(find dfc_mas_fr)/launch/node.launch">
            <arg name="name" value="node1"/>
            <arg name="id" value="1"/>
            <arg name="sim" value="$(arg sim)"/>
        </include>
