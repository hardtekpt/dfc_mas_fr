            <arg name="sim" value="$(arg sim)"/>
            <arg name="n_agents" value="$(arg n_agents)"/>
        </include> -->
        
        
        <!-- <node pkg="dfc_mas_fr" type="MarkerPublisher.py" name="marker_publisher" output="screen" args="node 9">
            
        </node> -->

        <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find dfc_mas_fr)/launch/rviz_cfg.rviz"/> -->
    </group>
</launch>